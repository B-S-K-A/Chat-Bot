{
      "name": "King Amda",
      "description": "An Anime themed Telegram group management bot.",
      "logo": "https://telegra.ph/file/3ea20a755f67a981eda42.jpg",
      "keywords": [
         "kingamda"
      ],   
   "repository": "https://github.com/King-Amda/Chat-Bot",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }     
  ],      
   "env": {
      "BOT_TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      }
   },
   "keywords": [
      "kingamda",
   ],
   "name": "KingAmda",
   "repository": "https://github.com/King-Amda/Chat-Bot"
}
